{
  "name": "vk-api-schema-parser",
  "version": "1.0.0",
  "description": "Parse JSON-Schema from the official git repository of VKontakte",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "postinstall": "tsc -p tsconfig.json",
    "wDebug": "@powershell -Command $env:DEBUG='*';node dist/index.js",
    "uDebug": "DEBUG=* node dist/index.js"
  },
  "dependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^17.0.22",
    "debug": "^4.3.4",
    "tsc": "^2.0.4",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "isolatedModules": true
    },
    "testMatch": [
      "<rootDir>/tests/**"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/utils"
    ]
  },
  "keywords": [
    "VK",
    "Code Generation",
    "Schema"
  ],
  "author": "Ripretention",
  "license": "ISC"
}
